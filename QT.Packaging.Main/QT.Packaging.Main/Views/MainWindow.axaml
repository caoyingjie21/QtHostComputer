<sukiUi:SukiWindow BackgroundStyle="Gradient"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:QT.Packaging.Main.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:sukiUi="clr-namespace:SukiUI.Controls;assembly=SukiUI"
        xmlns:views="clr-namespace:QT.Packaging.Main.Views"
        xmlns:materialIcons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
        mc:Ignorable="d" d:DesignWidth="1024" d:DesignHeight="768"
        x:Class="QT.Packaging.Main.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="桥头产线管理系统"
        Width="1024" Height="768"
        WindowState="Normal"
        CanResize="True">



    <!-- 对话框主机 -->
    <sukiUi:SukiWindow.Hosts>
        <sukiUi:SukiDialogHost Name="DialogHost"/>
    </sukiUi:SukiWindow.Hosts>

    <!-- 标题栏右侧控件移除设置按钮，避免重复 -->

	<views:MainView />
</sukiUi:SukiWindow>
